[test_case]
test = perfrunner.tests.magma.JavaDCPThroughputDGMTest

[showfast]
title = DCP Throughput, 4 nodes, 2B x 1KB, Java client, compression enabled, Magma
component = hidd
category = dcp

[stats]
client_processes = java

[bucket_extras]
backend = magma

[cluster]
mem_quota = 10240
initial_nodes = 4
num_buckets = 1
kernel_mem_limit = 20480M
kernel_mem_limit_services = kv

[bucket]
replica_number = 0
eviction_policy = fullEviction

[load]
items = 2000000000
size = 1024
workers = 128
key_fmtr = hex
doc_gen = large

[java_dcp]
config = perf/config/compression-enabled.properties
