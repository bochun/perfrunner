[test_case]
test = perfrunner.tests.magma.JavaDCPThroughputDGMTest

[showfast]
title = DCP Throughput, 4 nodes, 20B x 1KB, Java client, compression enabled, Magma
component = hidd
category = dcp

[stats]
client_processes = java

[cluster]
mem_quota = 10240
initial_nodes = 4
num_buckets = 1
kernel_mem_limit = 20G
kernel_mem_limit_services = kv

[bucket]
replica_number = 0
eviction_policy = fullEviction
backend_storage = magma

[load]
items = 20000000000
size = 1024
workers = 128
key_fmtr = hex
doc_gen = large

[java_dcp]
config = perf/config/compression-enabled.properties
