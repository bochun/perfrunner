[test_case]
test = perfrunner.tests.aerospike.YCSBThroughputTest

[showfast]
title = Avg Throughput (ops/sec), Workload A, 3 nodes
component = kv
category = ycsb
sub_category = ycsb3
orderby = a10

[cluster]
mem_quota = 32768
initial_nodes = 3
aerospike_conf = aerospike_32g.conf

[load]
workload_path = workloads/workload-asload
ycsb_client = aerospike
items = 400000000
workers = 275

[access]
workload_path = workloads/workload-asrun
ycsb_client = aerospike
items = 400000000
ops = 10000000000
time = 1800
workers = 275
workload_instances = 1
requestdistribution = uniform

[ycsb]
branch = master

[stats]
client_processes = java
