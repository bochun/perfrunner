[test_case]
test = perfrunner.tests.kv.MemoryOverheadTest

[showfast]
title = Memory overhead (%%), 1 bucket x 20M x 1KB, s=1000 c=1000
component = kv
category = memory

[stats]
interval = 1

[cluster]
mem_quota = 51200
initial_nodes = 4
num_buckets = 1

[collection]
config = collections/1bucket_1000scopes_1000collections_basic.json

[load]
items = 20000000
size = 1024
workers = 10

[access]
time = 300

[clients]
libcouchbase = 3.0.2
python_client = git+https://github.com/couchbase/couchbase-python-client@8d5820a297b35378d314eb399c041b214aa6a7db
