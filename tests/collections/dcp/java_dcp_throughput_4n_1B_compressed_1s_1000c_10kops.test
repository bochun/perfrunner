[test_case]
test = perfrunner.tests.dcp.JavaDCPCollectionThroughputTest

[showfast]
title = DCP Throughput, 1 node, 1B x 1KB, Java client, compression enabled, s=1 c=1000, 10k ops/sec
component = kv
category = dcp

[stats]
client_processes = java

[cluster]
mem_quota = 51200
initial_nodes = 4
num_buckets = 1

[bucket]
replica_number = 0
eviction_policy = fullEviction

[collection]
config = collections/1bucket_1scope_1000collections_basic.json

[load]
items = 1000000000
size = 1024
workers = 40
key_fmtr = hex
doc_gen = large

[access]
creates = 0
reads = 100
updates = 0
deletes = 0
items = 1000000000
workers = 24
throughput = 10000

[java_dcp]
config = perf/config/compression-enabled.properties
stream = all
clients = 1

[clients]
libcouchbase = 3.0.1
python_client = git+https://github.com/couchbase/couchbase-python-client@fd3879c31bf8f0737d776dee59fb9a54fc5b3ced

