[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = Avg Throughput (ops/sec), Workload CA, 3 nodes, 12 vCPU, uniform distribution, s=1 c=10
component = kv
category = ycsb
sub_category = ycsb3
orderby = a10

[cluster]
mem_quota = 40960
initial_nodes = 3
num_buckets = 1
online_cores = 6

[collection]
config = collections/1bucket_1scope_10collection_basic.json

[load]
workload_path = workloads/workloadca
ycsb_client = couchbase3
items = 20000000
workers = 100

[access]
workload_path = workloads/workloadca
ycsb_client = couchbase3
items = 20000000
ops = 1000000000
time = 1800
boost = 12
workers = 600
requestdistribution = uniform
workload_instances = 6
kv_endpoints = 16

[stats]
client_processes = java

[ycsb]
branch = couchbase3
