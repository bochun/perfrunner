[test_case]
test = perfrunner.tests.ycsb.YCSBDurabilityThroughputTest

[showfast]
title = Durability Persist Majority, 1 client, 128 threads, YCSB SLO, SDK 3, 4 nodes,  Workload CA, s=1 c=1000
component = kv
category = durability
sub_category = SLO
orderby = d311

[cluster]
mem_quota = 62000
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 2

[collection]
config = collections/1bucket_1scope_1000collections_basic.json

[load]
workload_path = workloads/workloadca
items = 20000000
workers = 100
ycsb_client = couchbase3

[access]
durability = 3
workload_path = workloads/workloadca
items = 20000000
ops = 100000000
time = 1200
workers = 128
workload_instances = 1
ycsb_client = couchbase3
enable_mutation_token = true
kv_endpoints = 64
timeseries = 1

[ycsb]
branch = couchbase3-collection-imp2
latency_percentiles = 98

[stats]
client_processes = java

