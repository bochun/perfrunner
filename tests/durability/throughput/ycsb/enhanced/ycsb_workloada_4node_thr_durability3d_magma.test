[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = Avg Throughput (ops/sec), SDK 3, Durability Persist Majority, 4 nodes,  Workload A, Magma
component = kv
category = durability
sub_category = Throughput
orderby = d31

[cluster]
mem_quota = 62000
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 2

[bucket_extras]
backend = magma

[load]
workload_path = workloads/workloada
items = 20000000
workers = 128
ycsb_client = couchbase3

[access]
durability = 3
workload_path = workloads/workloada
items = 20000000
ops = 100000000
time = 1200
workers = 128
workload_instances = 4
ycsb_client = couchbase3
enable_mutation_token = true
kv_endpoints = 64

[ycsb]
branch = couchbase3-new-durability