[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = Avg Throughput (ops/sec), SDK 3, replicate_to=0 persist_to=0, 4 nodes, Workload A
component = kv
category = durability
sub_category = Throughput
orderby = a30

[cluster]
mem_quota = 62000
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 2

[load]
workload_path = workloads/workloada
items = 20000000
workers = 128
ycsb_client = couchbase3

[access]
replicate_to = 0
persist_to = 0
workload_path = workloads/workloada
items = 20000000
ops = 100000000
time = 1200
workers = 128
workload_instances = 4
ycsb_client = couchbase3
enable_mutation_token = true
kv_endpoints = 8

[ycsb]
branch = couchbase3
