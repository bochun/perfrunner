[test_case]
test = perfrunner.tests.magma.YCSBThroughputHIDDTest

[showfast]
title = Avg Throughput (ops/sec), Workload G, 4 nodes, 1 bucket x 100M x 6KB, Uniform distribution, Magma
component = hidd
category = ycsb

[cluster]
mem_quota = 102400
initial_nodes = 4
num_buckets = 1

[bucket]
replica_number = 1
