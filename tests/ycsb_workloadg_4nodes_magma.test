[load]
workload_path = workloads/workloadg
ycsb_client = couchbase3
items = 100000000
workers = 160
field_length = 6000
field_count = 1

[access]
workload_path = workloads/workloadg
ycsb_client = couchbase3
items = 100000000
ops = 1000000000
time = 10800
boost = 12
workers = 160
workload_instances = 6
requestdistribution = uniform

[ycsb]
branch = couchbase3-new-durability

[stats]
client_processes = java