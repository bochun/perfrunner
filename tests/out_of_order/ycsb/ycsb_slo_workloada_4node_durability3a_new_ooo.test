[test_case]
test = perfrunner.tests.ycsb.YCSBDurabilityThroughputTest

[showfast]
title = Durability None, 1 client, 128 threads, YCSB SLO, SDK 3, 4 nodes,  Workload A, Out of Order
component = kv
category = durability
sub_category = SLO
orderby = d311

[cluster]
mem_quota = 62000
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 2

[load]
workload_path = workloads/workloada
items = 20000000
workers = 128
ycsb_client = couchbase3

[access]
durability = 0
workload_path = workloads/workloada
items = 20000000
ops = 100000000
time = 1200
workers = 128
workload_instances = 1
ycsb_client = couchbase3
enable_mutation_token = true
kv_endpoints = 8
timeseries = 1
out_of_order = 1

[ycsb]
branch = couchbase3
latency_percentiles = 98

